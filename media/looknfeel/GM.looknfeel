        </ImagerySection>
        <ImagerySection name="segment_hover">
            <ImageryComponent>
                <Area>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge" />
                            <ImageDim name="GM/HeaderBarSplitterNormal" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="GM/HeaderBarBackdropHover" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
